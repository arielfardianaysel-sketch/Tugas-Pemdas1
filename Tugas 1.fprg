    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="m, g, h, v, Ek, Ep, Em" type="Real" array="False" size=""/>
            <output expression="&quot;--- Program Menghitung Nilai EP, EK dan EM ---&quot;" newline="True"/>
            <output expression="&quot;Percepatan Gravitasi (m/s2)&quot;" newline="True"/>
            <input variable="g"/>
            <output expression="&quot;Massa Benda (Kg)&quot;" newline="True"/>
            <input variable="m"/>
            <output expression="&quot;Kecepatan Benda (m/s)&quot;" newline="True"/>
            <input variable="v"/>
            <output expression="&quot;Ketinggian Benda (m)&quot;" newline="True"/>
            <input variable="h"/>
            <assign variable="Ek" expression="(m*v*v)/2"/>
            <assign variable="Ep" expression="m*g*h"/>
            <assign variable="Em" expression="Ek+Ep"/>
            <output expression="&quot;Energi Potensial = &quot; &amp;Ep" newline="True"/>
            <output expression="&quot;Energi Kinetik = &quot; &amp;Ek" newline="True"/>
            <output expression="&quot;Energi Mekanik = &quot; &amp;Em" newline="True"/>
        </body>
    </function>
</flowgorithm>
